{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^3.1",
        "customfields/cep": "dev-develop",
        "customfields/cnpj": "*",
        "customfields/cpf": "*",
        "customfields/phone-number": "*",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/fortify": "^1.17",
        "laravel/framework": "^12.18",
        "laravel/horizon": "^5.33",
        "laravel/nova": "^5.0",
        "laravel/pint": "^1.25",
        "laravel/sanctum": "^4.1",
        "laravel/telescope": "^5.11",
        "laravel/tinker": "^2.8",
        "laravel/vapor-core": "^2.41",
        "league/flysystem-aws-s3-v3": "^3.28",
        "maatwebsite/excel": "^3.1",
        "psr/simple-cache": "^2.0",
        "sentry/sentry-laravel": "^4.15",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-permission": "^6.21"
    },
        "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/nova-devtool": "^1.8",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.8",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pint": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        "0": {
            "type": "path",
            "url": "./nova-components/PhoneNumber"
        },
        "1": {
            "type": "path",
            "url": "./nova-components/Cnpj"
        },
        "2": {
            "type": "path",
            "url": "./nova-components/Cpf"
        },
        "4": {
            "type": "path",
            "url": "./nova-components/Cep"
        }
    }
}
